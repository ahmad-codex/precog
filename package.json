{
  "name": "precogv3-smart-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "test:development": "truffle test test/test_development/case1.test.js",
    "deploy:staking:goerli": "hardhat run --network goerli deploy/staking_deploy.ts",
    "deploy:staking:fuji": "hardhat run --network fuji deploy/staking_deploy.ts",
    "deploy:staking:bsc_test": "hardhat run --network bsc_test deploy/staking_deploy.ts",
    "deploy:staking:mumbai": "hardhat run --network mumbai deploy/staking_deploy.ts",
    "verify:stakingFactory:goerli": "hardhat verify --network goerli --contract contracts/staking/StakingFactory.sol:StakingFactory --constructor-args deploy/stakingFactory.js",
    "verify:stakingPool:goerli": "hardhat verify --network goerli --contract contracts/staking/StakingPool.sol:StakingPool",
    "verify:rewardAPR:goerli": "hardhat verify --network goerli --contract contracts/staking/reward/APRRewardController.sol:APRRewardController",
    "verify:rewardFlexible:goerli": "hardhat verify --network goerli --contract contracts/staking/reward/FlexibleRewardController.sol:FlexibleRewardController",
    "verify:hr:goerli": "hardhat verify --network goerli  --contract contracts/common/auth/HumanResource.sol:HumanResource",
    "lint": "yarn lint:sol && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "prettier": "prettier --check --config ./.prettierrc.json \"**/*.{js,json,md,sol}\" --ignore-path .prettierignore",
    "prettier:format": "prettier --config ./.prettierrc.json --write \"**/*.{js,json,md,sol}\" --ignore-path .prettierignore",
    "migrate:goerli": "truffle migrate --network goerli -f 8 --to 8 --compile-none",
    "migrate:matic": "truffle migrate --network matic -f 8 --to 8 --compile-none",
    "migrate:avalanche_fuji": "truffle migrate --network avalanche_fuji -f 8 --to 8 --compile-none",
    "migrate:bsc_test": "truffle migrate --network bsc_test -f 8 --to 8 --compile-none",
    "migrate:development": "truffle migrate --network development -f 1 --to 1",
    "verify:kovan:faucet": "truffle run verify Faucet --network kovan",
    "verify:goerli": "truffle run verify PCOG TransparentUpgradeableProxy ProxyAdmin IPCOGFactory MiddlewareExchange PrecogStorage PrecogInternal PrecogCore PrecogFactory PrecogVault PrecogProfit PrecogV5 WithdrawalRegister --network goerli",
    "verify:matic": "truffle run verify PCOG TransparentUpgradeableProxy ProxyAdmin IPCOGFactory MiddlewareExchange PrecogStorage PrecogInternal PrecogCore PrecogFactory PrecogVault PrecogProfit PrecogV5 WithdrawalRegister --network matic",
    "verify:bsc_test": "truffle run verify PCOG TransparentUpgradeableProxy ProxyAdmin IPCOGFactory MiddlewareExchange PrecogStorage PrecogInternal PrecogCore PrecogFactory PrecogVault PrecogProfit PrecogV5 WithdrawalRegister --network bsc_test",
    "verify:avalanche_fuji": "truffle run verify PCOG TransparentUpgradeableProxy ProxyAdmin IPCOGFactory MiddlewareExchange PrecogStorage PrecogInternal PrecogCore PrecogFactory PrecogVault PrecogProfit PrecogV5 WithdrawalRegister --network avalanche_fuji",
    "flattener:pcog": "truffle-flattener ./contracts/pcog/PCOG.sol > ./flat/PCOG.flat.sol",
    "flattener:proxy-admin": "truffle-flattener ./contracts/pcog/ProxyAdmin.sol > ./flat/ProxyAdmin.flat.sol",
    "flattener:transparent-proxy": "truffle-flattener ./contracts/pcog/TransparentUpgradeableProxy.sol > ./flat/TransparentUpgradeableProxy.flat.sol",
    "flattener:precog-storage": "truffle-flattener ./contracts/precog/PrecogStorage.sol > ./flat/PrecogStorage.flat.sol",
    "flattener:precog-internal": "truffle-flattener ./contracts/precog/PrecogInternal.sol > ./flat/PrecogInternal.flat.sol",
    "flattener:precog-core": "truffle-flattener ./contracts/precog/PrecogCore.sol > ./flat/PrecogCore.flat.sol",
    "flattener:precog-factory": "truffle-flattener ./contracts/precog/PrecogFactory.sol > ./flat/PrecogFactory.flat.sol",
    "flattener:precog-vault": "truffle-flattener ./contracts/precog/PrecogVault.sol > ./flat/PrecogVault.flat.sol",
    "flattener:precog-profit": "truffle-flattener ./contracts/precog/PrecogProfit.sol > ./flat/PrecogProfit.flat.sol",
    "flattener:precog": "truffle-flattener ./contracts/precog/PrecogV5.sol > ./flat/PrecogV5.flat.sol",
    "flattener:withdrawal-register": "truffle-flattener ./contracts/withdrawal-register/WithdrawalRegister.sol > ./flat/WithdrawalRegister.flat.sol",
    "flattener:staking-factory": "truffle-flattener ./contracts/staking/StakingFactory.sol > ./flat/StakingFactory.flat.sol",
    "flattener:staking-pool": "truffle-flattener ./contracts/staking/StakingPool.sol > ./flat/StakingPool.flat.sol",
    "flattener:controller-apr": "truffle-flattener ./contracts/staking/reward/APRRewardController.sol > ./flat/APRRewardController.flat.sol",
    "flattener:controller-flexible": "truffle-flattener ./contracts/staking/reward/FlexibleRewardController.sol > ./flat/FlexibleRewardController.flat.sol",
    "flattener:human-resource": "truffle-flattener ./contracts/common/auth/HumanResource.sol > ./flat/HumanResource.flat.sol",
    "flattener:beacon-proxy": "truffle-flattener ./@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol > ./flat/BeaconProxy.flat.sol"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@openzeppelin/hardhat-upgrades": "^1.20.0",
    "@truffle/hdwallet-provider": "^2.0.12",
    "@types/jest": "^28.1.7",
    "big-number": "^2.0.0",
    "config": "^3.3.7",
    "dotenv": "^16.0.1",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-tx": "^2.1.2",
    "ethers": "^5.6.9",
    "solidity-coverage": "^0.7.21",
    "web3": "^1.7.4"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/truffle-upgrades": "^1.15.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.21.0",
    "hardhat": "^2.10.1",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-dev.23",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "truffle-contract-size": "^2.0.1",
    "truffle-plugin-verify": "^0.5.26",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  }
}